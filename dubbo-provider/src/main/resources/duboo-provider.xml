<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
       <dubbo:application name="demo-provider"/>
       <dubbo:registry address="zookeeper://localhost:2181?client=zkclient"/>
       <dubbo:protocol name="dubbo" port="-1"/>

       <dubbo:service interface="com.vivo.h5.dubbo.facade.DubboDemoServiceFacade" ref="dubboDemoServiceFacade"/>

       <bean id="dubboDemoServiceFacade" class="com.vivo.h5.dubbo.provider.DubboDemoServiceFacadeImpl"/>
</beans>